{
  "name": "CEH-Agent",
  "version": "0.1.12",
  "description": "CEH Agent is a Shell Socket Terminal allow full access to remotely run any app from server to local machine. Server socket sender and local socket listner can help to do navigation. Local socket can pass command to node server for exec() command to follow.",
  "main": "main.js",
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "scripts": {
    "postinstall": "node ./npm-node-os.js postinstall",
    "test": "echo \"Error: no test specified\" && exit 1",
    "copy": "node ./npm-node-os.js copy",
    "clean": "node ./npm-node-os.js clean",
    "build": "tsc && webpack  && node ./npm-node-os.js build",
    "webpack": "webpack && node build/index.js",
    "dev": "npm run build && node build/index.js",
    "start": "npm run build && node_modules/.bin/electron build/. --icns=build/assets/icons/mac/icon.icns",
    "package:mac": "cd build && yarn install --production=true && electron-packager . --overwrite --asar=true --platform=darwin --ignore=.bat --ignore=dev --arch=x64 --out=out "
  },
  "watch": {
    "test": "{src}/*.js"
  },
  "build": {
    "appId": "ceh.agent.adpcorp",
    "category": "agent.ceh.adpcorp.com",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "build/assets/icons/mac/icon.icns"
    },
    "win": {
      "target": "squirrel",
      "icon": "build/assets/icons/mac/icon.ico"
    }
  },
  "files": [
    "build/"
  ],
  "preferGlobal": "true",
  "author": "Nasir Sayed",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/nasihere/terminal-control.git"
  },
  "optionalDependencies": {
    "babel-preset-env": "^1.6.0",
    "d3": "^3.5.17",
    "d3-array": "^0.8.1",
    "d3-scale": "^0.9.3",
    "d3-shape": "^0.7.1",
    "d3-time-format": "^2.0.3",
    "file-loader": "^0.10.1",
    "prop-types": "^15.5.8",
    "react-addons-css-transition-group": "^15.4.2",
    "react-addons-transition-group": "^15.4.2",
    "react-easy-chart": "^0.1.13",
    "react-faux-dom": "^2.5.0",
    "react-markdown": "^2.5.0",
    "react-router": "^4.0.0",
    "reactstrap": "^4.3.0",
    "shebang": "0.0.1",
    "shebang-loader": "0.0.1",
    "ts-loader": "^2.0.3",
    "typescript": "^2.2.2"
  },
  "peerDependencies": {},
  "dependencies": {
    "@types/node": "^10.0.5",
    "@types/websocket": "0.0.33",
    "ansi-to-html": "^0.6.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.0.0-beta.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "electron": "^3.0.5",
    "electron-icon-maker": "^0.0.4",
    "css-loader": "^0.27.3",
    "electron-packager": "^12.2.0",
    "html-webpack-plugin": "^2.28.0",
    "copy-webpack-plugin": "^4.2.3",
    "react": "^15.5.4",
    "react-bootstrap": "^0.30.8",
    "react-dom": "^15.4.2",
    "react-lifecycle-component": "^3.0.0",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.14.1",
    "es6-promise": "^4.2.5",
    "url-loader": "^0.5.8",
    "ps-tree": "^1.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "asar": "^0.14.3",
    "webpack": "^3.11.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "websocket": "^1.0.24"
  },
  "devDependencies": {
  }
}
